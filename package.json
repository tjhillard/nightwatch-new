{
  "name": "nightwatch-test",
  "version": "1.0.0",
  "description": "Simple boilerplate for a new nightwatch.js project integrated with browserstack automate",
  "main": "index.js",
  "scripts": {
    "local": "nightwatch --config ./local.json",
    "browserstack-local": "node scripts/browserstack-local.js",
    "browserstack": "nightwatch -e browserstack-chrome,browserstack-firefox,browserstack-safari,browserstack-opera,browserstack-ie --config ./browserstack.json",
    "browserstack-chrome": "nightwatch -e browserstack-chrome --config ./browserstack.json",
    "browserstack-safari": "nightwatch -e browserstack-safari --config ./browserstack.json",
    "browserstack-opera": "nightwatch -e browserstack-opera --config ./browserstack.json",
    "browserstack-ie": "nightwatch -e browserstack-ie --config ./browserstack.json",
    "browserstack-firefox": "nightwatch -e browserstack-firefox --config ./browserstack.json"
  },
  "author": "TJ Hillard",
  "license": "ISC",
  "devDependencies": {
    "nightwatch": "^0.9.5"
  }
}
